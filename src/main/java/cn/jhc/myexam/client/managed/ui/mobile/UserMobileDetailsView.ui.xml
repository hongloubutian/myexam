<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    
    
    
    
    
    
    
    
    
    
    
    
    
    

    <g:Button ui:field="delete">Delete</g:Button>
  
        
        
    
        
        
        
    
        
        
        
        
    
        
        
        
        
        
    
        
        
        
        
        
        
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
        <div id="username">
            <div class="{style.label}">Username</div>
            <div class="{style.value}" ui:field="username"/>
        </div>
        <div id="password">
            <div class="{style.label}">Password</div>
            <div class="{style.value}" ui:field="password"/>
        </div>
        <div id="enabled">
            <div class="{style.label}">Enabled</div>
            <div class="{style.value}" ui:field="enabled"/>
        </div>
        <div id="displayName">
            <div class="{style.label}">Display Name</div>
            <div class="{style.value}" ui:field="displayName"/>
        </div>
        <div id="roles">
            <div class="{style.label}">Roles</div>
            <div class="{style.value}" ui:field="roles"/>
        </div>
        <div id="categories">
            <div class="{style.label}">Categories</div>
            <div class="{style.value}" ui:field="categories"/>
        </div>
        <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
