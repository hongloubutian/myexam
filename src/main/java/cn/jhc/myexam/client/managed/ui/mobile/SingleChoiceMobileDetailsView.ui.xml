<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    

    <g:Button ui:field="delete">Delete</g:Button>
  
        
        
    
        
        
        
    
        
        
        
        
    
        
        
        
        
        
    
        
        
        
        
        
        
    
        
        
        
        
        
        
        
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
        <div id="question">
            <div class="{style.label}">Question</div>
            <div class="{style.value}" ui:field="question"/>
        </div>
        <div id="optionA">
            <div class="{style.label}">Option A</div>
            <div class="{style.value}" ui:field="optionA"/>
        </div>
        <div id="optionB">
            <div class="{style.label}">Option B</div>
            <div class="{style.value}" ui:field="optionB"/>
        </div>
        <div id="optionC">
            <div class="{style.label}">Option C</div>
            <div class="{style.value}" ui:field="optionC"/>
        </div>
        <div id="optionD">
            <div class="{style.label}">Option D</div>
            <div class="{style.value}" ui:field="optionD"/>
        </div>
        <div id="answer">
            <div class="{style.label}">Answer</div>
            <div class="{style.value}" ui:field="answer"/>
        </div>
        <div id="category">
            <div class="{style.label}">Category</div>
            <div class="{style.value}" ui:field="category"/>
        </div>
        <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
