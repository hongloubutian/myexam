<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    

    <g:Button ui:field="delete">Delete</g:Button>
  
        
        
    
        
        
        
    
        
        
        
        
    
        
        
        
        
        
    
        
        
        
        
        
        
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
        <div id="name">
            <div class="{style.label}">Name</div>
            <div class="{style.value}" ui:field="name"/>
        </div>
        <div id="info">
            <div class="{style.label}">Info</div>
            <div class="{style.value}" ui:field="info"/>
        </div>
        <div id="timeOpen">
            <div class="{style.label}">Time Open</div>
            <div class="{style.value}" ui:field="timeOpen"/>
        </div>
        <div id="timeClose">
            <div class="{style.label}">Time Close</div>
            <div class="{style.value}" ui:field="timeClose"/>
        </div>
        <div id="content">
            <div class="{style.label}">Content</div>
            <div class="{style.value}" ui:field="content"/>
        </div>
        <div id="category">
            <div class="{style.label}">Category</div>
            <div class="{style.value}" ui:field="category"/>
        </div>
        <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
