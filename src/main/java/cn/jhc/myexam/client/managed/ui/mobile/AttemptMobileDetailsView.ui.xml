<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    
    
    
    
    
    
    
    
    
    
    
    
    
    

    <g:Button ui:field="delete">Delete</g:Button>
  <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
        <div id="questionType">
            <div class="{style.label}">Question Type</div>
            <div class="{style.value}" ui:field="questionType"/>
        </div>
        <div id="answer">
            <div class="{style.label}">Answer</div>
            <div class="{style.value}" ui:field="answer"/>
        </div>
        <div id="submitDate">
            <div class="{style.label}">Submit Date</div>
            <div class="{style.value}" ui:field="submitDate"/>
        </div>
        <div id="user">
            <div class="{style.label}">User</div>
            <div class="{style.value}" ui:field="user"/>
        </div>
        <div id="quiz">
            <div class="{style.label}">Quiz</div>
            <div class="{style.value}" ui:field="quiz"/>
        </div>
        <div id="questionId">
            <div class="{style.label}">Question Id</div>
            <div class="{style.value}" ui:field="questionId"/>
        </div>
        <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
